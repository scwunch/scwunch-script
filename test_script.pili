function joiner
	[list|tuple ls, str sep]:
	    if len[ls] == 0
	        return ''
		result = ''
		for s in ls[0,-2]
			result += s.str + sep
		return result + ls[-1].str  # [0, result.len-sep.len]
	[list|tuple ls]:
		return joiner[ls, ' ']

# print my_list.joiner

fsf[float f1*, str s*, float f2*]:
	result = ''
	if f1
		result = "f1: " + f1.joiner
	if s
		result += ', '*bool[result] + "s: " + s.join[' ']
	if f2
		result += ', '*bool[result] + "f2: " + f2.joiner
	return result

